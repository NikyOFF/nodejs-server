{
  "name": "nodejs-backend",
  "version": "2.1.0",
  "description": "Nodejs backend server template",
  "main": "lib/index.js",
  "types": "lib",
  "scripts": {
    "start": "concurrently \"node ./lib/index.js\" \"npm run agendash\"",
    "dev": "concurrently \"nodemon -r tsconfig-paths/register src/index.ts\" \"npm run agendash\"",
    "agendash": "npx agendash2 --db=mongodb://localhost/discord_core --collection=_agenda --port=4001",
    "build": "rm -rf ./lib/**/*.js && tsc --project tsconfig.json && tsc-alias -p tsconfig.json"
  },
  "author": "nikyoff",
  "license": "MIT",
  "dependencies": {
    "agenda": "^4.2.1",
    "agendash2": "^0.8.2",
    "bcryptjs": "^2.4.3",
    "celebrate": "^15.0.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "dotenv": "^10.0.0",
    "event-dispatch": "^0.4.1",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.0.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.3",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.2.0",
    "tsc-alias": "^1.3.10",
    "typedi": "^0.10.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/agenda": "^4.1.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/csurf": "^1.11.2",
    "@types/express": "^4.17.13",
    "@types/helmet": "^4.0.0",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.10.1",
    "@types/nodemailer": "^6.4.4",
    "@types/nodemon": "^1.19.1",
    "@types/passport": "^1.0.7",
    "@types/passport-jwt": "^3.0.6",
    "@types/reflect-metadata": "^0.1.0",
    "concurrently": "^6.3.0",
    "nodemon": "^2.0.12",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.3"
  }
}
